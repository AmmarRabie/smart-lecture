<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cmp.sem.team8.smarlecture.joinsession.JoinedSession">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/joinedsession_write_attendance"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:onClick="onToFragmentClick"
            android:text="Write Attendance" />



    </LinearLayout>

    <LinearLayout
        android:id="@+id/Linear_layout"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:id="@+id/joinedsession_contentFrame"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />




    </LinearLayout>




</LinearLayout>
