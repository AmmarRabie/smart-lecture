<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cmp.sem.team8.smarlecture.home.groups.GroupsActivity"
    tools:layout_editor_absoluteY="81dp">

    <Button
        android:id="@+id/testForceLogin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:onClick="testForceLogin"
        android:text="test force login"
        app:layout_constraintStart_toStartOf="@+id/testProfile"
        app:layout_constraintTop_toBottomOf="@+id/testProfile" />

    <Button
        android:id="@+id/testProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="49dp"
        android:layout_marginTop="16dp"
        android:onClick="testProfile"
        android:text="test profile"
        app:layout_constraintBottom_toTopOf="@+id/coordinatorLayout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.502"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/quick_beginsession"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Begin session"
        android:onClick="beginSession"
        app:layout_constraintStart_toStartOf="@+id/testForceLogin"
        app:layout_constraintTop_toBottomOf="@+id/testForceLogin" />


    <Button
        android:id="@+id/quick_connectsession"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:text="Connect Session"
        android:onClick="connectSession"
        app:layout_constraintStart_toStartOf="@+id/quick_beginsession"
        app:layout_constraintTop_toBottomOf="@+id/quick_beginsession" />

    <android.support.design.widget.CoordinatorLayout
        android:id="@+id/coordinatorLayout"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginBottom="1dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/testProfile">



        <FrameLayout
            android:id="@+id/contentFrame"
            android:layout_width="384dp"
            android:layout_height="399dp"
            tools:layout_editor_absoluteY="-3dp" />


    </android.support.design.widget.CoordinatorLayout>


</android.support.constraint.ConstraintLayout>
